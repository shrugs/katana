datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          String   @id @default(uuid())
  address     String
  discordId   String?
  username    String?
  katanas     String[]
  inServer    Boolean  @default(false)
  lastChecked DateTime @default(now())
}
